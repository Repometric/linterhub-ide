{
	"extends": [
		"tslint-eslint-rules"
	],
	"rules": {
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": ["always"],
		"triple-equals": true,
		"no-consecutive-blank-lines": true,
		"eofline": true,
        "completed-docs": [true, {
			"classes": {
				"visibilities": ["exported"]
			},
			"enums": {
				"visibilities": ["exported"]
			},
			"functions": {
				"visibilities": ["exported"]
			},
			"interfaces": {
				"visibilities": ["exported"]
			},
			"methods": {
				"locations": ["all"],
				"privacies": ["public", "protected"]
			},
			"properties": {
				"locations": ["all"],
				"privacies": ["public", "protected"]
			},
			"types": {
				"visibilities": ["exported"]
			},
			"variables": {
				"visibilities": ["exported"]
			}
		}]
	}
}
