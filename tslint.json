{
	"extends": [
		"tslint-eslint-rules"
	],
	"rules": {
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": ["always"],
		"triple-equals": true,
        "completed-docs": [true, {
			"classes": {
				"visibilities": ["exported"]
			},
			"functions": {
				"visibilities": ["exported"]
			},
			"methods": {
				"locations": ["instance"],
				"privacies": ["public", "protected"]
			}
		}]
	}
}